{
    "name": "popcorn",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
      "-------BASE--------": "",
      "compile": "npx hardhat compile",
      "------DEPLOY------": "",
      "deploy:avalanche": "npx hardhat --network avalanche deploy",
      "deploy:fuji": "npx hardhat --network avalancheFujiTestnet deploy",
      "deploy:boba": "npx hardhat --network boba deploy",
      "-----UPGRADE-----": "",
      "upgrade:avalanche": "npx hardhat --network avalanche upgrade",
      "upgrade:fuji": "npx hardhat --network avalancheFujiTestnet upgrade",
      "upgrade:boba": "npx hardhat --network boba upgrade",
      "-------VERIFY------": "",
      "verify:avalanche": "npx hardhat --network avalanche verify",
      "verify:fuji": "npx hardhat --network avalancheFujiTestnet verify",
      "verify:boba": "npx hardhat --network boba verify",
      "----AVALANCHE----": "",
      "cosmic:avalanche:deploy": "npm run deploy:avalanche -- --name CosmicUpgradeable",
      "cosmic:avalanche:upgrade": "npm run upgrade:avalanche -- --name CosmicUpgradeable",
      "cosmic:avalanche:verify": "npm run verify:avalanche -- --name CosmicUpgradeable",
      
      "-------BOBA--------": "",
      "cosmicBadges:boba:deploy": "npm run deploy:boba -- --name CosmicBadges",
      "cosmicBadges:boba:upgrade": "npm run upgrade:boba -- --name CosmicBadges",
      "cosmicBadges:boba:verify": "npm run verify:boba -- --name CosmicBadges",

      "-------FUJI--------": "",
      "cosmicBadges:fuji:deploy": "npm run deploy:fuji -- --name CosmicBadges",
      "cosmicBadges:fuji:upgrade": "npm run upgrade:fuji -- --name CosmicBadges",
      "cosmicBadges:fuji:verify": "npm run verify:fuji -- --name CosmicBadges"
    },
    "author": "dirtycajunrice",
    "license": "MIT",
    "dependencies": {
      "@dirtycajunrice/hardhat-tasks": "^1.3.0",
      "@openzeppelin/contracts": "4.8.0",
      "dotenv": "16.0.3",
      "hardhat": "2.12.2",
      "ts-node": "10.9.1",
      "typescript": "4.9.3"
    },
    "devDependencies": {
      "@chainlink/contracts": "0.5.1",
      "@dirtycajunrice/contracts": "0.10.0",
      "@ethersproject/experimental": "^5.7.0",
      "@openzeppelin/contracts-upgradeable": "4.8.0"
    }
  }